            "language": "typescript",
            "source": [
                "function combineText(name: string, ...texts: string[]) {\n  console.log(texts.length, \" input\");\n  console.log(`${name} : ${texts.join(\",\")}`);\n}\n\ncombineText(\"title\", \"a\", \"b\", \"c\", \"d\");\n"
            ],
