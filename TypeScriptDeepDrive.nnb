            "language": "typescript",
            "source": [
                "// @ts-nocheck\n\nconsole.log(5 == \"5\"); // true\nconsole.log(5 === \"5\"); // false\n\nconsole.log(\"\" == \"0\"); // false\nconsole.log(0 == \"\"); // true\n\nconsole.log(\"\" === \"0\"); // false\nconsole.log(0 === \"\"); // false\n"
